{
  "name": "ngx-jsonapi",
  "version": "0.0.3-rc.3",
  "description": "JSON API library for Angular",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "ts-node ./build/index.ts",
    "postbuild": "rimraf **/dist/**/*.ngsummary.json",
    "deploy:builds": "ts-node ./build/deploy-build.ts",
    "test:unit": "node ./tests.js",
    "test": "nyc yarn run test:unit",
    "clean": "git clean -xdf && yarn && yarn run bootstrap",
    "cli": "ng",
    "coverage:html": "nyc report --reporter=html",
    "copy:dist": "ncp dist/ ./node_modules/ngx-jsonapi/",
    "start": "yarn run demo:start",
    "demo:start": "yarn run build && yarn run copy:dist && yarn run cli serve",
    "demo:start:aot": "yarn run build && yarn run copy:dist && yarn run cli serve --aot",
    "demo:test": "jest --watch",
    "demo:build": "yarn build && yarn run copy:dist && yarn cli build -prod --base-href \"/\" --output-path \"./demo-dist\"",
    "demo:publish": "ts-node ./build/demo-publish.ts",
    "ci": "yarn run build && yarn run test && nyc report --reporter=text-lcov | coveralls",
    "watch:tests": "chokidar 'src/**/*.ts' --initial -c 'nyc --reporter=text --reporter=html yarn run test:unit'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "cd dist && yarn publish",
    "codegen": "ts-node src/codegen/src/index.ts"
  },
  "engines": {
    "node": ">=8.6.0",
    "npm": ">=5.3.0",
    "yarn": ">=1.3.0 <2.0.0"
  },
  "keywords": [
    "jsonapi",
    "angular",
    "angular2",
    "angular4"
  ],
  "authors": [
    "Pablo Gabriel Reyes"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/reyesoft/ngx-jsonapi"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec",
      "**/spec/**/*",
      "/src/schematics/src/*/files/**/*",
      "**/schematics/src/utility/*"
    ],
    "include": [
      "**/*.ts"
    ]
  },
  "devDependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/cli": "^1.5.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/platform-server": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@types/fs-extra": "^2.1.0",
    "@types/glob": "^5.0.33",
    "@types/jasmine": "2.5.45",
    "@types/jasminewd2": "^2.0.2",
    "@types/jest": "^20.0.2",
    "@types/lodash": "^4.14.80",
    "@types/node": "^7.0.5",
    "@types/ora": "^1.3.1",
    "@types/rimraf": "^0.0.28",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^2.1.1",
    "conventional-changelog": "^1.1.4",
    "core-js": "^2.4.1",
    "coveralls": "^2.13.0",
    "cpy-cli": "^1.0.1",
    "deep-freeze": "^0.0.1",
    "fs-extra": "^2.1.2",
    "gh-pages": "^1.1.0",
    "glob": "^7.1.2",
    "hammerjs": "^2.0.8",
    "jasmine": "^2.5.3",
    "jasmine-core": "~2.5.2",
    "jasmine-marbles": "^0.0.2",
    "jasmine-spec-reporter": "~3.2.0",
    "jest": "^21.0.2",
    "jest-preset-angular": "^3.0.1",
    "jest-zone-patch": "^0.0.7",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lerna": "^2.0.0",
    "module-alias": "^2.0.0",
    "ncp": "^2.0.0",
    "nyc": "^10.1.2",
    "ora": "^1.3.0",
    "prettier": "^1.8.2",
    "protractor": "~5.1.0",
    "reflect-metadata": "^0.1.9",
    "rimraf": "^2.5.4",
    "rollup": "^0.50.0",
    "sorcery": "^0.10.0",
    "ts-node": "^3.1.0",
    "tslib": "1.6.0",
    "tslint": "^4.4.2",
    "typescript": "~2.4.0",
    "uglify-js": "^3.1.9",
    "zone.js": "^0.8.12"
  },
  "dependencies": {
    "localforage": "^1.5.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jest.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "demo/tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/demo/**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "html",
      "json"
    ],
    "mapCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/*.*/"
    ],
    "moduleNameMapper": {
      "^ngx-jsonapi/(?!db)(.*)": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!ngx-jsonapi)"
    ],
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
